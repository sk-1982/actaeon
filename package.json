{
  "name": "actaeon",
  "version": "0.1.0",
  "private": true,
  "license": "0BSD",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "(export ANALYZE=true || set ANALYZE=true) && npm run build",
    "start": "next start",
    "lint": "next lint",
    "migrate:up": "dotenvx run -f .env.local -- node db-migrate.cjs up",
    "migrate": "npm run migrate:up",
    "migrate:down": "dotenvx run -f .env.local -- node db-migrate.cjs down",
    "migrate:create": "dotenvx run -f .env.local -- db-migrate create",
    "db:export": "dotenvx run -f .env.local -- kysely-codegen --exclude-pattern cozynet* --exclude-pattern alembic_version --exclude-pattern actaeon_migrations --out-file src/types/db.d.ts && node process-db.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "@next/bundle-analyzer": "^14.2.0",
    "@nextui-org/react": "^2.2.10",
    "@tanstack/react-virtual": "^3.2.1",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.0",
    "db-migrate": "^0.11.14",
    "db-migrate-mysql": "^2.3.2",
    "deep-is": "^0.1.4",
    "framer-motion": "^11.0.28",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.3",
    "mysql2": "^3.9.4",
    "next": "14.2.0",
    "next-auth": "^5.0.0-beta.15",
    "next-client-cookies": "^1.1.1",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18",
    "react-grid-layout": "^1.4.4",
    "react-icons": "^5.0.1",
    "react-resizable": "^3.0.5",
    "react-swipeable": "^7.0.1",
    "sass": "^1.75.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-text-fill-stroke": "^2.0.0-beta.1",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^0.32.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-resizable": "^3.0.7",
    "autoprefixer": "^10.4.19",
    "eslint": "^8",
    "eslint-config-next": "14.2.0",
    "kysely-codegen": "^0.14.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "type-fest": "^4.15.0",
    "typescript": "^5"
  }
}
